import java.util.*; 
import java.io.*;

class Function {  
  int FirstFactorial(int num) { 
  	int count = num;
    int sum=1;
    for(int i =0;i<num;i++){
     sum = sum * count;
     count--;
    }
       
    return sum;
    
  } 
  
  public static void main (String[] args) {  
    // keep this function call here     
    Scanner  s = new Scanner(System.in);
    Function c = new Function();
    System.out.print(c.FirstFactorial(s.nextLine())); 
  }   
  
}           

